{
  "name": "nodejs-express-mongodb",
  "version": "1.0.0",
  "description": "rest api",
  "main": "./dist/server/generated.js",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "build": "webpack --mode=production --config webpack.config.server.js",
    "dev": "NODE_ENV=cloud nodemon",
    "deploy": "npm run build && gcloud app deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rest",
    "api"
  ],
  "author": "Eli Menchaca",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "5.10.13"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "file-loader": "^5.1.0",
    "nodemon": "2.0.2",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
